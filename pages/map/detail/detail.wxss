
.page {
  width:100%;
}

.page__title {
  display: flex;
  flex-flow:row nowrap;
  justify-content:center;
  align-items: center;
  text-align: center;
  height: 42px;
  line-height: 42px;
  border-bottom: 1px solid #d0d0d0;
}

.page__title text {
  font-size: 11pt;
  width: 60%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.box{
  display: flex;
  flex-direction: row;
  flex-flow: nowrap;
  justify-content: flex-start;
}

.box-item{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-start;
  width: 200px;
}


.weui-cell_access{
  height: 18px;
  line-height: 18px;
}
text {
  color: #888;
  font-size: 10pt;
}
.weui-cells {
  margin-top: 0px;
}
swiper-item {
  text-align: center;
}
.slide-image {
  width: 100%;
  height: 150px;
}
