/* pages/project/projectWho/projectWho.wxss */
.whoMenu{
    margin: 0rpx 10rpx;
    border-bottom: 1rpx solid #D9D9D9;
    border-top: 1rpx solid #D9D9D9;
    background-color: #FFFFFF;
    height: 30px;
    line-height: 30px;
    display: flex;
}
.itmes{
    display: flex;
    width: 20%;
    align-items:center;
    justify-content:center; 
}
.whoMenu image{
    margin-right: 5px;
    vertical-align: middle;
    width:15px;
    height: 15px;
    margin-bottom: 10rpx;
}
.section{
    background-color: #FFFFFF;
    padding-left: 50rpx;
    border-bottom: 1rpx solid #D9D9D9;
    border-top: 1rpx solid #D9D9D9;
}
.labelClass{
    display: flex;
    padding: 40rpx 0rpx;
    border-top: 1rpx solid #D9D9D9;
    line-height: 60rpx;
}
.labelClass:before{
    content:" ";
    border-top: 1rpx solid #D9D9D9;
}
.radioClass{
    display: flex;
}
.userInfos{
    display: flex;
    width: 100%;
}